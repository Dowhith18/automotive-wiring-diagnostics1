{
  "name": "automotive-wiring-diagnostics",
  "version": "1.0.0",
  "description": "Professional automotive diagnostic system inspired by ZEUS+ with AI-powered fault detection",
  "author": "Automotive Diagnostics Team",
  "license": "MIT",
  "homepage": "https://Dowhith18.github.io/automotive-wiring-diagnostics1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dowhith18/automotive-wiring-diagnostics1.git"
  },
  "keywords": [
    "automotive",
    "diagnostics", 
    "obd2",
    "react",
    "zeus",
    "ford",
    "wiring",
    "ml",
    "tensorflow"
  ],
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "@mui/material": "^5.11.10",
    "@mui/icons-material": "^5.11.9",
    "@mui/x-charts": "^5.15.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "chart.js": "^4.2.1",
    "react-chartjs-2": "^5.2.0",
    "@tensorflow/tfjs": "^4.2.0",
    "@tensorflow/tfjs-react-native": "^0.8.0",
    "axios": "^1.3.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.6",
    "framer-motion": "^10.0.1",
    "react-spring": "^9.6.1",
    "workbox-webpack-plugin": "^6.5.4",
    "react-virtualized": "^9.22.5",
    "react-window": "^1.8.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "prettier": "^2.8.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "cypress": "^12.7.0",
    "gh-pages": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:e2e": "cypress open",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "docker:build": "docker build -t automotive-diagnostics .",
    "docker:run": "docker run -p 3000:3000 automotive-diagnostics"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:3001",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}